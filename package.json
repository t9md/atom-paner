{
  "name": "paner",
  "main": "./lib/main",
  "version": "0.2.1",
  "description": "Pane manipulation helper",
  "activationCommands": {
    "atom-workspace": [
      "paner:maximize",
      "paner:swap-item",
      "paner:send-item",
      "paner:merge-item",
      "paner:very-top",
      "paner:very-bottom",
      "paner:very-left",
      "paner:very-right",
      "paner:split-up",
      "paner:split-down",
      "paner:split-right",
      "paner:split-left"
    ]
  },
  "repository": "https://github.com/t9md/atom-paner",
  "license": "MIT",
  "engines": {
    "atom": ">=0.174.0 <2.0.0"
  },
  "dependencies": {
    "underscore-plus": "^1.6.6"
  },
  "configSchema": {
    "debug": {
      "type": "boolean",
      "default": false
    },
    "mergeSameOrientaion": {
      "type": "boolean",
      "default": true,
      "description": "When moving very far, merge child PaneAxis to Parent if orientaion is same"
    }
  }
}
